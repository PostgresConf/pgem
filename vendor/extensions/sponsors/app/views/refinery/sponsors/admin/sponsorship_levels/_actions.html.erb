<ul>
  <% if ::Refinery::Sponsors::Admin::SponsorshipLevelsController.searchable? %>
    <li>
      <%= render '/refinery/admin/search', :url => refinery.sponsors_admin_sponsorship_levels_path %>
    </li>
  <% end %>
  <li>
    <%= action_label :add, refinery.new_sponsors_admin_sponsorship_level_path, t('.create_new') %>
  </li>
<% if !searching? && ::Refinery::Sponsors::Admin::SponsorshipLevelsController.sortable? && ::Refinery::Sponsors::SponsorshipLevel.many? %>
  <li>
    <%= action_label :reorder,
                     refinery.sponsors_admin_sponsorship_levels_path,
                     t('.reorder', what:  "Sponsorship Levels"), id: 'reorder_action'%>
    <%= action_label :reorder_done,
                     refinery.sponsors_admin_sponsorship_levels_path,
                      t('.reorder_done', what: "Sponsorship Levels"), id: 'reorder_action_done', class: 'hidden'%>
  </li>
<% end %>
</ul>
